# Paths managed by Composer
/wp
/wp-content/vendor
/wp-content/plugins/*
/wp-content/themes/*

# Override to include my own themes/plugins
!/wp-content/plugins/rnf*
!/wp-content/themes/rnf*

# My own wp-config files, except the sample
/wp-config*.php
!/wp-config-sample.php

# Content directories
wp-content/uploads

# Auto-generated stuff
wp-content/upgrade
wp-content/themes/rnf-alfa/sources/img/headers
wp-content/themes/rnf-alfa/dist
sitemap.xml
sitemap.xml.gz
*.log

# Package manager and compiler stuff
.sass-cache
node_modules

# OS Generated stuff (from Pantheon's .gitignore)
.DS_Store*
ehthumbs.db
Icon?
Thumbs.db
._*
*.un~
.git-checkout-commit-hash
