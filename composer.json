{
  "name": "tsmith512/routenotfound",
  "description": "Route Not Found. Site root and custom code repo.",
  "authors": [
    {
      "name": "Taylor Smith",
      "homepage": "https://www.tsmithcreative.com"
    }
  ],
  "type": "project",
  "repositories": [
    {
      "type": "composer",
      "url" : "https://wpackagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "fancyapps/fancybox",
        "version": "3.5.6",
        "source": {
          "url": "https://github.com/fancyapps/fancybox.git",
          "type": "git",
          "reference": "v3.5.6"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "filamentgroup/loadCSS",
        "version": "2.0.1",
        "source": {
          "url": "https://github.com/filamentgroup/loadCSS.git",
          "type": "git",
          "reference": "v2.0.1"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "mapbox/mapbox.js-bower",
        "version": "3.1.1",
        "source": {
          "url": "https://github.com/mapbox/mapbox.js-bower.git",
          "type": "git",
          "reference": "v3.1.1"
        }
      }
    }
  ],
  "config": {
    "vendor-dir": "wp-content/vendor"
  },
  "require": {
    "johnpbloch/wordpress": "=5.5.1"
  },
  "require-dev": {
    "wpackagist-theme/twentyseventeen":"~2.0",
    "wpackagist-plugin/tumblr-importer":"0.8",
    "wpackagist-plugin/wp-cfm": "1.6",
    "wpackagist-plugin/autoptimize":"2.4.4",
    "wpackagist-plugin/rocket-lazy-load":"2.1.3",
    "wpackagist-plugin/postie":"1.9.53",
    "fancyapps/fancybox":"3.5.x",
    "filamentgroup/loadcss":"2.x.x",
    "mapbox/mapbox.js-bower":"3.1.x"
  },
  "extra": {
    "wordpress-install-dir": "wp"
  },
  "scripts": {}
}
